    dialogue: "\u4E26\u6C92\u6709\u3002"
  - type: 1
    id: 4
    chara: 1
    dialogue: "\u54C8\u2014\uFF1F\u524D\u9663\u5B50\u4E0D\u662F\u624D\u63A5\u4E86\u500B\u5831\u916C\u9084\u4E0D\u932F\u59D4\u8A17\u800C\u5DF2\uFF0C\u4EBA\u5BB6\u624D\u4E0D\u8981\u52D2\uFF01"
  - type: 1
    id: 5
    chara: 2
    dialogue: "\u4F46\u59B3\u6240\u8AAA\u7684\u5831\u916C\u4F3C\u4E4E\u5FEB\u8981\u898B\u5E95\u4E86\uFF0C\u5404\u4F4D\u6709\u4EC0\u9EBC\u982D\u7DD2\u55CE?"
  - type: 1
    id: 6
    chara: 3
    dialogue: "\u982D\u7DD2\u55CE\u2026\u2026\uFF0C\u6211\u53EA\u6709\u53BB\u6700\u8FD1\u65B0\u958B\u5E55\u7684\u9AD8\u7D1A\u9910\u5EF3\u5927\u5403\u4E00\u9813\u800C\u5DF2\u3002"
  - type: 1
    id: 7
    chara: 1
    dialogue: "\u6211\u4E5F\u53EA\u662F\u8CB7\u4E86\u4E00\u9EDE\u5316\u599D\u54C1\u7292\u8CDE\u81EA\u5DF1\u800C\u5DF2\u2026\u2026"
  - type: 1
    id: 8
    chara: 1
    dialogue: "\u8A71\u8AAA\u5929\u6D6A\u4F60\u623F\u9593\u4E0D\u662F\u4E5F\u591A\u4E86\u4E00\u5806\u7684\u66F8\u55CE\uFF1F"
